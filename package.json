{
  "name": "@flossbank/registry-resolver",
  "version": "2.2.0",
  "description": "Resolve list of packages into weighted map of dependencies",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "test": "npm run unit-test && ts-standard",
    "unit-test": "ava",
    "lint": "ts-standard --fix",
    "coverage": "nyc npm run unit-test"
  },
  "dependencies": {
    "@snyk/ruby-semver": "^3.0.2",
    "call-limit": "^1.1.1",
    "got": "^11.8.1",
    "npm-package-arg": "^8.0.1",
    "pacote": "^11.1.11"
  },
  "devDependencies": {
    "@types/npm-package-arg": "^6.1.0",
    "@types/pacote": "^11.1.0",
    "@types/sinon": "^10.0.1",
    "ava": "^3.15.0",
    "nyc": "^15.1.0",
    "sinon": "^9.0.3",
    "ts-node": "^10.0.0",
    "ts-standard": "^10.0.0",
    "typescript": "^4.3.2"
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nonSemVerExperiments": {
      "configurableModuleFormat": true
    },
    "files": [
      "test/*"
    ],
    "nodeArguments": [
      "--loader=ts-node/esm"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flossbank/registry-resolver.git"
  },
  "keywords": [],
  "author": "Peter Squicciarini <stripedpajamas273@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/flossbank/registry-resolver/issues"
  },
  "homepage": "https://github.com/flossbank/registry-resolver#readme"
}
